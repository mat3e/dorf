{
  "name": "dorf",
  "version": "3.2.2",
  "description": "Domain Object Reactive Forms for Angular and TypeScript",
  "keywords": [
    "decorator",
    "annotation",
    "form",
    "angular2",
    "angular4",
    "html",
    "ts",
    "forms",
    "reactive",
    "dynamic",
    "model-driven",
    "typescript",
    "angular",
    "object",
    "domain",
    "decorators",
    "annotations"
  ],
  "scripts": {
    "patch": "npm run build && npm test && npm version patch",
    "test": "karma start config/karma/conf.js",
    "api-doc": "node_modules/.bin/typedoc --theme default --experimentalDecorators --mode file --target \"es6\" --module \"commonjs\" --name \"DORF Documentation\" --excludeNotExported --excludePrivate --out docs/api src",
    "build": "ngc",
    "build-all": "npm run build && npm run build-examples",
    "build-examples": "npm run build-old-examples && npm run build-new-examples",
    "build-old-examples": "npm run field-example && npm run extras-example && npm run definition-example",
    "field-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/new-field/node_modules/dorf/",
    "extras-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/definition-extras/node_modules/dorf/",
    "definition-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/different-definition/node_modules/dorf/",
    "build-new-examples": "npm run simple-example-new && npm run nested-complex-example",
    "simple-example-new": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/simple/node_modules/dorf/",
    "nested-complex-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/nested-complex/node_modules/dorf/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat3e/dorf"
  },
  "author": "Mateusz Chrzonstowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mat3e/dorf/issues"
  },
  "homepage": "https://mat3e.github.io/dorf/",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "devDependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/platform-server": "^4.3.0",
    "@angular/tsc-wrapped": "4.3.0",
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.7",
    "angular2-template-loader": "0.6.2",
    "copyfiles": "1.2.0",
    "core-js": "2.4.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "jasmine-core": "2.6.4",
    "karma": "1.7.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.2",
    "style-loader": "0.18.2",
    "ts-loader": "2.2.2",
    "tslint": "5.5.0",
    "typedoc": "0.7.1",
    "typescript": "~2.4.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.5.0",
    "webpack-merge": "4.1.0",
    "zone.js": "0.8.12"
  }
}